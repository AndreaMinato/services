{
  "presets": [
    "flow",
    [
      "env",
      {
        "modules": "commonjs",
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        },
        "exclude": ["transform-regenerator"]
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "ramda",
    "transform-async-to-generator"
  ]
}
